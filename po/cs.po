#: ../src/calc.c:1961 ../src/calc.c:1970 ../src/calc.c:1975 ../src/calc.c:1984
#: ../src/calc.c:1989 ../src/calc.c:2000 ../src/calc.c:2005 ../src/calc.c:2016

msgstr ""

#: C/genius.xml:4916(term)
msgid "<anchor id=\"gel-function-IsPerfectSquare\"/>IsPerfectSquare"
msgstr ""

#: C/genius.xml:4918(synopsis)
#, no-wrap
msgid "IsPerfectSquare (n)"
msgstr "IsPerfectSquare (n)"


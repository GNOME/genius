#: C/genius.xml:3510(term)
msgid "SmithNormalFormInteger"
msgstr "SmithNormalFormInteger"

#: C/genius.xml:3512(synopsis)
#, no-wrap
msgid "SmithNormalFormInteger (M)"
msgstr "SmithNormalFormInteger (M)"


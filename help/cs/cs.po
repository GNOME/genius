"        if m@(j,i) == 0 then\n"
"          (j=j+1;continue);\n"
"        a := m@(j,);\n"
"        m@(j,) := m@(d,);\n"
"        m@(d,) := a;\n"
"        j := j+1;\n"
"        break\n"
"      )\n"
"    );\n"
"    if m@(d,i) == 0 then\n"
"      (i:=i+1;continue);\n"
"    \n"
"    # Here comes the actual zeroing of all but the anchor\n"
"    # element rows\n"
"    j := d+1;\n"
"    while j &lt;= rows(m)) do (\n"
"      if m@(j,i) != 0 then (\n"
"        m@(j,) := m@(j,)-(m@(j,i)/m@(d,i))*m@(d,)\n"
"      );\n"
"      j := j+1\n"
"    );\n"
"    m@(d,) := m@(d,) * (1/m@(d,i));\n"
"    d := d+1;\n"
"    i := i+1\n"
"  );\n"
"  m\n"
")\n"

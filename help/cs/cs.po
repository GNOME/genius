msgstr "<anchor id=\"gel-function-Factorial\"/>Factorial"

#: C/genius.xml:6813(synopsis)
#, no-wrap
msgid "Factorial (n)"
msgstr "Factorial (n)"

#: C/genius.xml:6814(para)

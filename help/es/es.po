msgstr "Obtener el entero más alto menor o igual que <varname>n</varname>."

#: C/genius.xml:4154(term)
msgid "<anchor id=\"gel-function-ln\"/>ln"
msgstr "<anchor id=\"gel-function-ln\"/>ln"


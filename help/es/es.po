#, no-wrap
msgid "ref (M)"
msgstr "ref (M)"

#: C/genius.xml:3684(para)
msgid "Aliases: <function>REF</function><function>RowEchelonForm</function>"
msgstr "Alias: <function>REF</function><function>RowEchelonForm</function>"


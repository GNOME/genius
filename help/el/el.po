msgstr "<anchor id=\"gel-function-StirlingNumberSecond\"/>StirlingNumberSecond"

#: C/genius.xml:6870(synopsis)
#, no-wrap
msgid "StirlingNumberSecond (n,m)"
msgstr "StirlingNumberSecond (n,m)"

#: C/genius.xml:6871(para)

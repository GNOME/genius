msgstr "<anchor id=\"gel-function-StringToAlphabet\"/>StringToAlphabet"

#: C/genius.xml:8310(synopsis)
#, no-wrap
msgid "StringToAlphabet (str,alphabet)"
msgstr "StringToAlphabet (str,alphabet)"

